{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": 0,
		"react/prop-types": 0,
		"comma-dangle": ["warn", "never"]
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
